---
layout: _default
---
{% if title and showTitle != false %}
  <h1>{{ title }}</h1>
{% endif %}

{% if pageDescription %}
  {% if pageDescription %}
    {% for text in pageDescription %}
      <p>{{ text }}</p>
    {% endfor %}
  {% else %}
    <p>{{ pageDescription }}</p>
  {% endif %}
{% endif %}

{{ content }}